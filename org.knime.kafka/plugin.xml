<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/labs/kafka"
            factory-class="org.knime.kafka.node.producer.KafkaProducerNodeFactory">
      </node>
      <node
            category-path="/labs/kafka"
            deprecated="false"
            factory-class="org.knime.kafka.node.consumer.KafkaConsumerNodeFactory">
      </node>
      <node
            category-path="/labs/kafka"
            deprecated="false"
            factory-class="org.knime.kafka.node.loops.start.LoopStartKafkaNodeFactory">
      </node>
      <node
            category-path="/labs/kafka"
            deprecated="false"
            factory-class="org.knime.kafka.node.loops.end.LoopEndKafkaNodeFactory">
      </node>
      <node
            category-path="/labs/kafka"
            deprecated="false"
            factory-class="org.knime.kafka.node.connector.KafkaConnectorNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/apache_kafka-icon.png"
            level-id="kafka"
            name="Kafka"
            path="/labs/">
      </category>
   </extension>
   <extension
         point="org.knime.core.PortType">
      <portType
            color="#42b0f4"
            hidden="false"
            name="KafkaConnection"
            objectClass="org.knime.kafka.port.KafkaConnectorPortObject"
            objectSerializer="org.knime.kafka.port.KafkaConnectorPortObject$Serializer"
            specClass="org.knime.kafka.port.KafkaConnectorPortSpec"
            specSerializer="org.knime.kafka.port.KafkaConnectorPortSpec$Serializer">
      </portType>
   </extension>
</plugin>
